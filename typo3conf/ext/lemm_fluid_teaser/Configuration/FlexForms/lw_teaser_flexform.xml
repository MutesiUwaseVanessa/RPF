<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>1</langChildren>
	</meta>
	<ROOT>
		<type>array</type>
		<el>
			<options.isWorldTeaser>
				<TCEforms>
					<exclude>1</exclude>
					<label>
						LLL:EXT:lemm_fluid_teaser/Resources/Private/Language/locallang_be.xlf:flexform.isWorldTeaser
					</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</options.isWorldTeaser>
			<options.title>
				<TCEforms>
					<exclude>1</exclude>
					<label>
						LLL:EXT:lemm_fluid_teaser/Resources/Private/Language/locallang_be.xlf:flexform.title
					</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</options.title>
			<options.link>
				<TCEforms>
					<exclude>1</exclude>
					<label>
						LLL:EXT:lemm_fluid_teaser/Resources/Private/Language/locallang_be.xlf:flexform.link
					</label>
					<config>
						<type>input</type>
						<size>30</size>
						<eval>trim</eval>
						<softref>typolink,typolink_tag,images,url</softref>
						<wizards>
							<_PADDING>2</_PADDING>
							<link>
								<type>popup</type>
								<title>Link</title>
								<module>
									<name>wizard_element_browser</name>
									<urlParameters>
										<mode>wizard</mode>
									</urlParameters>
								</module>
								<icon>link_popup.gif</icon>
								<script>browse_links.php?mode=wizard</script>
								<params>
									<!--<blindLinkOptions>page,file,folder,url,spec</blindLinkOptions>-->
								</params>
								<JSopenParams>height=500,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
							</link>
						</wizards>
					</config>
				</TCEforms>
			</options.link>
			<options.description>
				<TCEforms>
					<exclude>1</exclude>
					<label>
						LLL:EXT:lemm_fluid_teaser/Resources/Private/Language/locallang_be.xlf:flexform.description
					</label>
					<config>
						<type>text</type>
						<cols>10</cols>
						<rows>5</rows>
					</config>
					<defaultExtras>richtext[*]:rte_transform[flag=rte_enabled|mode=ts_css]</defaultExtras>
				</TCEforms>
			</options.description>
		</el>
	</ROOT>
</T3DataStructure>